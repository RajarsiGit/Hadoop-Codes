package q1;

import java.util.*;
public class HahMapsSample {
	public static void main(String args[]) {
		HashMap <Integer,String> c = new HashMap<Integer,String>();
		c.put(1,"core");
		c.put(2,"java");
		c.put(3,"session");
		System.out.println(c.containsValue("session")+" "+c.containsValue(1));
		}
	}